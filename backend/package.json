{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start:dev": "yarn build && yarn prisma delete -f && yarn prisma reset -f && yarn prisma deploy --force && node dist/src/index.js",
