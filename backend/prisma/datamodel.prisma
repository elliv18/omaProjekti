type User {
  id: ID! @id
  type: String!
  name: String!,
  email: String! @unique,
  password: String!
}
type Vinyl {
  id: ID! @id
  name: String!
  artists: [Artist!]! @relation(name: "ArtistsVinyl")
}
type Artist {
  id: ID! @id
  name: String!
  vinyls: [Vinyl]  @relation(name: "ArtistsVinyl")
}
